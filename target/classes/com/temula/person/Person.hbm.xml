<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.jboss.org/dtd/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.temula.person">
    <class name="Person" table="person">
        <id name="personId" column="prsn_id">
            <generator class="native"/>
        </id>
        <property  not-null="true" name="firstName" column="prsn_fname"/>
        <property  not-null="true" name="lastName" column="prsn_lname"/>
    </class>
</hibernate-mapping>
